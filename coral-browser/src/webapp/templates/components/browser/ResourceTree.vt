$page_tool.addScriptLink("js/PopupMenu.js")
$page_tool.addStyleLink("style/popupmenu.css")

#set($tableLink = $link.set('table_id', $table.id))
<table class="genericItemTree" cellpadding="0" cellspacing="0" width="100%">
  <tr>
    <th>Resource tree</th>
  </tr>
#foreach($row in $table.rows)
  <tr>
    <td>
      #tableView_linesAndFolders($table $row $tableLink)
      <a href="$link.set('res_id',$row.object.id)">$row.object.name</a>
    </td>
  </tr>
#end
</table>
