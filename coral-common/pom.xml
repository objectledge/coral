        <timezone>+1</timezone>
        <roles>
            <role>retired developer</role>
        </roles>
    </developer>
  </developers>
  <scm>
    <connection>scm:svn:http://objectledge.org/svn/coral</connection>
    <developerConnection>scm:svn:http://objectledge.org/svn/coral</developerConnection>
    <url>http://objectledge.org/fisheye/browse/coral</url>
  </scm>
  <issueManagement>
    <system>Jira</system>
    <url>http://objectledge.org/jira/browse/CORAL</url>
  </issueManagement>
  <ciManagement>
    <system>Hudson</system>
    <url>http://objectledge.org/hudson/view/coral</url>
  </ciManagement>
  <properties>
    <versions.coral>1.0.0-SNAPSHOT</versions.coral>
  </properties>
  <modules>
    <module>../coral-api</module>
    <module>../coral-ri</module>
    <module>../coral-datatypes</module>
    <module>../coral-tools</module>
    <module>../coral-maven2-plugin</module>
    <module>../coral-web</module>
    <module>../coral-browser</module>
    <module>../coral-touchstone</module>
  </modules>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.objectledge.coral</groupId>
        <artifactId>coral-api</artifactId>
        <version>${versions.coral}</version>
      </dependency>
      <dependency>
        <groupId>org.objectledge.coral</groupId>
        <artifactId>coral-ri</artifactId>
        <version>${versions.coral}</version>
      </dependency>
      <dependency>
        <groupId>org.objectledge.coral</groupId>
        <artifactId>coral-datatypes</artifactId>
        <version>${versions.coral}</version>
      </dependency>
      <dependency>
        <groupId>org.objectledge.coral</groupId>
        <artifactId>coral-web</artifactId>
        <version>${versions.coral}</version>
      </dependency>
      <dependency>
        <groupId>org.objectledge.coral</groupId>
        <artifactId>coral-maven2-plugin</artifactId>
        <version>${versions.coral}</version>
        <type>maven-plugin</type>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <dependencies>
    <dependency>
      <groupId>jmock</groupId>
      <artifactId>jmock</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>jmock</groupId>
      <artifactId>jmock-cglib</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>cglib</groupId>
      <artifactId>cglib-full</artifactId>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>nexus.objectledge.org</id>
      <name>ObjectLedge releases Nexus repository</name>
      <url>http://objectledge.org/nexus/content/repositories/m2releases</url>
    </repository>
    <snapshotRepository>
      <id>nexus.objectledge.org</id>
      <name>ObjectLedge snapshots Nexus repository</name>
      <url>http://objectledge.org/nexus/content/repositories/m2snapshots</url>
    </snapshotRepository>
  </distributionManagement>
</project>